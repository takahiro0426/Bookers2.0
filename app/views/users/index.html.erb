
<%= render partial: 'users/user_mene', locals: { user: @user } %>

<div class="col-9">
	<h2>Users</h2>
	<div class="col9">
		<table class="table">
			<thead>
			<tr><th>image</th>
				<th>name</th>
				<th></th>
			</tr>
		</thead>
		<% @users.each do |user| %>
		<tr>
			<th><%= attachment_image_tag user,:profile_image, :fill, 40, 40, fallback: "no_image.jpg", size:'40x40', class: " pull-left" %></th>
			<th><%= user.name %> </th>
			<th><%= "Show" %></th>
		</tr>
		<% end %>
		</table>
	</div>
</div>
